{
	"name": "meaningful",
	"displayName": "Meaningful",
	"description": "An extension to find quickly search the meaning on a highlighted piece of code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.meaningful"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.meaningful",
			"title": "Meaningful Search"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
